{
  "name": "@elzup/renovate-config",
  "version": "1.2.0",
  "main": "index.js",
  "license": "MIT",
  "files": [
    "index.js",
    "package.json",
    "nodep.json"
  ],
  "renovate-config": {
    "default": {
      "extends": [
        "config:base",
        ":automergeLinters",
        ":automergeMinor",
        ":automergeTypes",
        ":disableDependencyDashboard",
        ":ignoreModulesAndTests",
        ":label(renovate)",
        ":maintainLockFilesMonthly",
        ":rebaseStalePrs",
        ":timezone(Asia/Tokyo)",
        "group:allNonMajor",
        "group:definitelyTyped",
        "group:linters",
        "group:monorepos",
        "schedule:monthly"
      ]
    }
  }
}
